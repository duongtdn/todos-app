<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.todostogether" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TodosTogether</name>
    <description>
        A collaborative Todo Application
    </description>
    <author email="duongtdn@stormgle.com" href="http://stormgle.com">
        Stormgle's Eagle Squad
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="browser" spec="~4.1.0" />
    <plugin name="com.admob.plugin" spec="~5.5.0" />
</widget>
